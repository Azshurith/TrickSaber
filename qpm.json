{
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "Trick Saber",
    "id": "trick-saber",
    "version": "0.0.1",
    "url": "https://github.com/RedBrumbler/TrickSaber",
    "additionalData": {
      "overrideSoName": "libtrick-saber.so"
    }
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^3.14.0",
      "additionalData": {
        "extraFiles": [
          "src/inline-hook"
        ]
      }
    },
    {
      "id": "codegen",
      "versionRange": "^0.33.0",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.15.23",
      "additionalData": {}
    },
    {
      "id": "modloader",
      "versionRange": "^1.2.3",
      "additionalData": {}
    },
    {
      "id": "paper",
      "versionRange": "^1.2.9",
      "additionalData": {}
    },
    {
      "id": "lapiz",
      "versionRange": "^0.1.11",
      "additionalData": {}
    },
    {
      "id": "sombrero",
      "versionRange": "^0.1.37",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.3.1",
      "additionalData": {}
    },
    {
      "id": "bs-utils",
      "versionRange": "^0.7.2",
      "additionalData": {}
    },
    {
      "id": "conditional-dependencies",
      "versionRange": "^0.1.0",
      "additionalData": {}
    }
  ],
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "clean": [
        "pwsh ./scripts/build.ps1 -clean"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1"
      ],
      "log": [
        "pwsh ./scripts/log.ps1"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1 -clean",
        "qpm qmod build",
        "pwsh ./scripts/createqmod.ps1 TrickSaber -clean"
      ]
    }
  }
}